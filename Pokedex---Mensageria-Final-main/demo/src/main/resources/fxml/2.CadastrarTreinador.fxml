<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="470.0" prefWidth="401.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cliente.controllers.CadastrarTreinadorController">
    <children>
        <TextField fx:id="nomeTreinadorField" layoutX="122.0" layoutY="91.0" promptText="Nome do treinador" />
        <TextField fx:id="nivelTreinadorField" layoutX="122.0" layoutY="134.0" promptText="Nível do treinador" />

        <Text fx:id="pokedexTitulo" layoutX="74.0" layoutY="52.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Cadastro de treinadores">

            <font>
                <Font size="24.0" />
            </font>
        </Text>
        <Button fx:id="salvarButton" layoutX="122.0" layoutY="181.0" mnemonicParsing="false" onAction="#salvarTreinador" text="Salvar" />

        <Button fx:id="voltarButton" layoutX="223.0" layoutY="181.0" mnemonicParsing="false" onAction="#voltarInicio" text="Voltar" />
      <TableView layoutX="84.0" layoutY="235.0" prefHeight="200.0" prefWidth="225.0">
        <columns>
          <TableColumn fx:id="idTreinadorColuna" prefWidth="75.0" text="Id" />
          <TableColumn fx:id="nomeColuna" prefWidth="75.0" text="Nome" />
            <TableColumn fx:id="nivelColuna" prefWidth="75.0" text="Nível" />
        </columns>
      </TableView>
    </children>
</AnchorPane>
